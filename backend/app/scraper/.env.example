# Example environment configuration for the scraper
# Copy this to .env and fill in your actual values

# Database connection
DATABASE_URL=postgresql://user:password@localhost/partle

# Store IDs (get these from your database)
BRICODEPOT_STORE_ID=4064
LEROY_MERLIN_STORE_ID=4065
FERRETERIAS_STORE_ID=4066

# Default user ID for scraped products (system/bot user)
SCRAPER_USER_ID=1

# Scraping behavior
ENABLE_DUPLICATE_FILTER=true
UPDATE_EXISTING_PRODUCTS=true

# Logging
SCRAPER_LOG_LEVEL=INFO